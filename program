let n = 0;
let e = 0;
let boxes = [1, 0, 2, 0, 3, 4, 0, 5];
for (let i = 0; i < boxes.length; i++) {
    if (boxes[i] !== 0) {
      boxes[n] = boxes[i];
      n++;
    } else {
      e++;
    }
  }

while (n < boxes.length) {
    boxes[n] = 0;
    n++;
  }
console.log("Updated Boxes:", boxes);
console.log("Count of Empty Boxes:", e);
